import java.util.*;
class one {
    public void lolita(){
       System.out.println("hi i am big sis"); 
    }
}
class two extends one{
    public void lili(){
        System.out.println("hi i am lolita's sister");
    }
}
class three extends one{
    public void lola(){
        System.out.println("hi i am the smallest one of lili and lolita");
    }
}
public class Main{
    public static void main(String[] args){
       three input = new three();
       two input1 = new two();
       input1.lili();
       input1.lolita();
       input.lola();
       input.lolita();
    }
    
}
